          "questions": [
            {
              "id": "8.A.1",
              "text": "Кашель, поднятие тяжестей, напряжение сопровождаются непроизвольным мочеиспусканием",
              "options": "0148"
            },
            {
              "id": "8.A.2",
              "text": "Боль средней степени в нижней части спины",
              "options": "0148"
            },
            {
              "id": "8.A.3",
              "text": "Боль в нижней части живота",
              "options": "0148"
            },
            {
              "id": "8.A.4",
              "text": "Боль или жжение при мочеиспускании",
              "options": "0148"
            },
            {
              "id": "8.A.5",
              "text": "Редкие позывы к мочеиспусканию",
              "options": "0148"
            },
            {
              "id": "8.A.6",
              "text": "Позывы чаще, чем раз в 2 часа ночью и днем",
              "options": "0148"
            },
            {
              "id": "8.A.7",
              "text": "Сильный запах мочи",
              "options": "0148"
            },
            {
              "id": "8.A.8",
              "text": "Боль в спине и ногах сразу после мочеиспускания",
              "options": "0148"
            },
            {
              "id": "8.A.9",
              "text": "Болезненные ощущения в области гениталий",
              "options": "0148"
            },
            {
              "id": "8.A.10",
              "text": "Моча розового цвета",
              "options": "0148"
            },
            {
              "id": "8.A.11",
              "text": "Внезапная потребность в испражнении вызывает непроизвольное мочеиспускание",
              "options": "0148"
            },
            {
              "id": "8.A.12",
              "text": "Ощущение задержки мочи, которое чувствуется во всем теле",
              "options": "0148"
            }
          ]
        }
