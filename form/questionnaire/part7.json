{
  "part": 7,
  "title": "Часть 7 — Глаза/уши/нос/горло/дыхание",
  "sections": [
    {
      "code": "A",
      "title": "Секция A",
      "items": [
        { "code": "7.A.1", "text": "Влажные глаза или слезы", "type": "scale_0_1_4_8" },
        { "code": "7.A.2", "text": "Выделение слизи из глаз", "type": "scale_0_1_4_8" },
        { "code": "7.A.3", "text": "Боль в ушах/застой/зуд", "type": "scale_0_1_4_8" },
        { "code": "7.A.4", "text": "Выделения из ушей", "type": "scale_0_1_4_8" },
        { "code": "7.A.5", "text": "Постоянно заложен нос", "type": "scale_0_1_4_8" },
        { "code": "7.A.6", "text": "Склонны к громкому храпу", "type": "yes_no_0_8" },
        { "code": "7.A.7", "text": "Частый насморк", "type": "scale_0_1_4_8" },
        { "code": "7.A.8", "text": "Бывает кровь из носа", "type": "yes_no_0_8" },
        { "code": "7.A.9", "text": "Грубый голос", "type": "scale_0_1_4_8" },
        { "code": "7.A.10", "text": "Часто откашливаетесь", "type": "scale_0_1_4_8" },
        { "code": "7.A.11", "text": "Чувствуется комок в горле", "type": "scale_0_1_4_8" },
        { "code": "7.A.12", "text": "Сильно простужаетесь", "type": "yes_no_0_8" },
        { "code": "7.A.13", "text": "Чувствуете себя скверно всю зиму", "type": "yes_no_0_8" },
        { "code": "7.A.14", "text": "Симптомы гриппа длятся >5 дней", "type": "yes_no_0_8" },
        { "code": "7.A.15", "text": "Инфекции поражают легкие", "type": "yes_no_0_8" },
        { "code": "7.A.16", "text": "Дискомфорт/боль в грудной клетке", "type": "scale_0_1_4_8" },
        { "code": "7.A.17", "text": "Внезапное затруднение дыхания", "type": "scale_0_1_4_8" },
        { "code": "7.A.18", "text": "Одышка", "type": "scale_0_1_4_8" },
        { "code": "7.A.19", "text": "Трудности при вдохе", "type": "scale_0_1_4_8" },
        { "code": "7.A.20", "text": "Даже небольшое напряжение вызывает кашель", "type": "scale_0_1_4_8" },
        { "code": "7.A.21", "text": "Трудно дышать лежа", "type": "scale_0_1_4_8" },
        { "code": "7.A.22", "text": "Отхаркиваете много слизи при кашле", "type": "scale_0_1_4_8" },
        { "code": "7.A.23", "text": "Вдох и выдох сопровождаются шумом", "type": "scale_0_1_4_8" },
        { "code": "7.A.24", "text": "Беспокоит кашель", "type": "scale_0_1_4_8" },
        { "code": "7.A.25", "text": "Затрудненное шумное дыхание", "type": "scale_0_1_4_8" },
        { "code": "7.A.26", "text": "Сильно потеете ночью", "type": "scale_0_1_4_8" },
        { "code": "7.A.27", "text": "Голубой оттенок губ/ног", "type": "scale_0_1_4_8" },
        { "code": "7.A.28", "text": "Дневная сонливость", "type": "scale_0_1_4_8" },
        { "code": "7.A.29", "text": "Трудно сконцентрироваться", "type": "scale_0_1_4_8" },
        { "code": "7.A.30", "text": "Симптомы (глаза/уши/нос/горло/легкие) вызваны продуктами (молоко, пшеница)", "type": "yes_no_0_8" },
        { "code": "7.A.31", "text": "Симптомы из п. 30 вызваны сезонными изменениями", "type": "yes_no_0_8" }
      ]
    }
  ]
}

